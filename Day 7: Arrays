
𝐏𝐫𝐨𝐛𝐥𝐞𝐦----

𝐈𝐧 𝐡𝐚𝐜𝐤𝐞𝐫𝐫𝐚𝐧𝐤 𝐩𝐫𝐚𝐜𝐭𝐢𝐜𝐞 𝐬𝐞𝐬𝐬𝐢𝐨𝐧 𝐨𝐟 𝟑𝟎 𝐃𝐚𝐲𝐬 𝐨𝐟 𝐜𝐨𝐝𝐞.




𝐒𝐨𝐥𝐮𝐭𝐢𝐨𝐧𝐬----

#include <bits/stdc++.h>

using namespace std;

int main ()
{
    vector<int>v1;
    int n,y,i;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>y;
        v1.push_back(y);
    }
    for(i=n-1;i>=0;i--)
    {
       cout<<v1[i]<<" ";
    }
    return 0;
}

#include <bits/stdc++.h>

using namespace std;

int main ()
{
    vector<int>v1;
    int n,y,i;
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>y;
        v1.push_back(y);
    }
    for(i=n-1;i>=0;i--)
    {
       cout<<v1[i]<<" ";
    }
    return 0;
}
